{
  "extends": ["airbnb"],
  "plugins": ["prettier"],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  "rules": {
    "no-undef": 0,
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "object-curly-newline": 0,
    "no-confusing-arrow": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "jsx-a11y/label-has-associated-control": 0,
    "jsx-a11y/control-has-associated-label": 0
  }
}
